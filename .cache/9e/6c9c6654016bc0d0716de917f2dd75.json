original":{"line":149,"column":2},"generated":{"line":148,"column":2}}],"sources":{"register.css":"@import url(\"../css/navbar.css\");\r\n@import url('https://fonts.googleapis.com/css?family=Raleway&display=swap');\r\n\r\n*{\r\n    padding: 0;\r\n    border: 0;\r\n    font-family: 'Raleway', sans-serif;\r\n}\r\nbody{\r\n    \r\n}\r\n.bg{\r\n    width: 100%;\r\n    height: 100vh;\r\n    position: relative;\r\n    background: linear-gradient(rgba(0,0,0,0.6),rgba(0,0,0,0.9)), url('../images/form-bg.jpg') center/cover;\r\n}\r\n.form-container{\r\n    display: flex;\r\n    width: 750px;\r\n    height: 550px;\r\n    position: relative;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    box-shadow: 0 20px 50px rgba(0, 0, 0, .4);\r\n    /* opacity: 1;  */\r\n}\r\n.sign-up-form{\r\n   background: #232328;\r\n   width: 50%;\r\n   height: 100%;\r\n   padding: 50px 50px;\r\n   box-sizing: border-box;\r\n}\r\n.form-input {\r\n    width: 300px;\r\n    color: #cc683c;\r\n    background: transparent;\r\n    border: none;\r\n    border-bottom: 2px solid white;\r\n    align-items: center;\r\n    outline: none;\r\n    margin-bottom: 50px;\r\n    padding: 10px 0;\r\n    letter-spacing: 1px;\r\n    font-size: 18px;\r\n    font-family: 'Oswald', sans-serif;\r\n    transition: border-bottom-color .8s;\r\n}\r\n.form-input:focus{\r\n    border-bottom-color: #cc673c52;\r\n}\r\n.sign-up-form p{\r\n    color: white;\r\n    font-family: 'Oswald', sans-serif;\r\n    margin-bottom: 8px;\r\n    font-size: 18px;\r\n    letter-spacing: 1px;\r\n    font-weight: 100;\r\n}\r\n.sign-up-form label{\r\n    color: #ffffff;\r\n    font-family: 'Oswald', sans-serif;\r\n    font-size: 15px;\r\n    font-weight: 200;\r\n    letter-spacing: 1px;\r\n    text-transform: uppercase;\r\n}\r\n.sign-up-form input[type =\"checkbox\"]{\r\n    position: relative;\r\n    appearance: none;\r\n    -webkit-appearance: none;\r\n    -moz-appearance: none;\r\n    width:30px;\r\n    height: 15px;\r\n    background: white;\r\n    border-radius: 50px;\r\n    margin:0 8px;\r\n    box-shadow: inset 0 0 10px rgba(112, 25, 25, 0.4);\r\n    outline: none;\r\n    transition: background-color .5s;\r\n}\r\n.sign-up-form input:checked[type=\"checkbox\"]{\r\n    background-color: #cc683c;\r\n}\r\n\r\n.sign-up-form input[type=\"checkbox\"]::before{\r\n    content: \"\";\r\n    width: 17px;\r\n    height: 17px;\r\n   background-color: #aaaaaa;\r\n   position: absolute;\r\n   top: -1px;\r\n   left: -4px;\r\n   border-radius: 50%;\r\n   box-shadow: 0 2px 5px rgba(0, 0, 0, .4);\r\n   transition: left .5s;\r\n}\r\n.sign-up-form input:checked[type=\"checkbox\"]::before{\r\n    left: 18px;\r\n}\r\n.form-btn{\r\n    display: block;\r\n    margin-top: 0px;\r\n    width: 150px;\r\n    font-size: 18px;\r\n    font-family: 'Oswald', sans-serif;\r\n    background: transparent;\r\n    outline: none;\r\n    border-radius: 50px;\r\n    color: #ffffff;\r\n    padding: 8px 5px ;\r\n    border: 2px solid white;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    cursor: pointer;\r\n}\r\n\r\n.header{\r\n    width: 50%;\r\n    background: linear-gradient(to top, rgba(0, 0, 0, .4), rgba(0, 0, 0, .2)), url(\"../images/bg2.jpeg\") no-repeat center;  \r\n    background-size: cover;\r\n    box-shadow: -10px 0 30px rgba(0,0,0,0.3);\r\n    position: relative;\r\n  }\r\n  .header h1{\r\n      position: absolute;\r\n      top: 25%;\r\n      padding: 15px;\r\n      color:white;\r\n  }\r\n  .x-btn{\r\n      position: absolute;\r\n      bottom: -3.5%;\r\n      right: 1%;\r\n      font-size: 20px;\r\n      font-weight: bold;\r\n      color: #262626;\r\n      cursor: pointer;\r\n  }\r\n  .form-btn a{\r\n      color: white;\r\n      text-decoration: none;\r\n      cursor: pointer;\r\n      letter-spacing: 1px;\r\n      font-size: 15px;\r\n      width: 150px;\r\n  }\r\n  "},"lineCount":null}},"error":null,"hash":"fd3c7ad7a7dc428c2701f9f92cfed1a3","cacheData":{"env":{}}}